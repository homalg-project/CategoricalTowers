    end );
    
    ##
    AddRightUnitorInverseWithGivenTensorProduct( FpMatAlg_k,
      function( FpMatAlg_k, fp_matrix_algebra, target )
        local FpAlg_k;
        
        FpAlg_k := UnderlyingCategoryOfFpAlgebras( FpMatAlg_k );
        
